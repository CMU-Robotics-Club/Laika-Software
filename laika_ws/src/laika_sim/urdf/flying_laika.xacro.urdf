<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="laika">
  <!-- MATERIALS -->
  <material name="body">
    <color rgba="0.6 0.3 0.3 1"/>
  </material>
  <material name="hip">
    <color rgba="0.8 0.4 0.7 1"/>
  </material>
  <material name="upper">
    <color rgba="0.5 0.2 0.1 1"/>
  </material>
  <material name="lower">
    <color rgba="0.3 0.4 0.6 1"/>
  </material>
  <material name="feet">
    <color rgba="0.8 0.2 0.1 1"/>
  </material>

  <!-- CONSTANTS -->
  <xacro:property name="damping" value="0.2" />
  <xacro:property name="friction" value="0.01" />

  <!-- Make Laika fly in the air -->
  <link name="world"/>
  <joint name="world_to_base" type="fixed">
    <origin rpy="0 0 ${pi}" xyz="0 0 1"/>
    <parent link="world"/>
    <child link="base"/>
  </joint>

  <!-- SPHERES AT THE END -->
  <link name="fl_sphere">
    <visual>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
      <material name="feet"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </collision>
  </link>
  <link name="fr_sphere">
    <visual>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
      <material name="feet"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </collision>
  </link>
  <link name="bl_sphere">
    <visual>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
      <material name="feet"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </collision>
  </link>
  <link name="br_sphere">
    <visual>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
      <material name="feet"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </collision>
  </link>
  <joint name="fl_foot" type="fixed">
    <origin rpy="0 0 0" xyz="-0.4 -0.05 0.015"/>
    <parent link="fl_lower_link"/>
    <child link="fl_sphere"/>
  </joint>
  <joint name="fr_foot" type="fixed">
    <origin rpy="0 0 0" xyz="-0.4 -0.05 0.015"/>
    <parent link="fr_lower_link"/>
    <child link="fr_sphere"/>
  </joint>
  <joint name="bl_foot" type="fixed">
    <origin rpy="0 0 0" xyz="-0.4 -0.05 0.015"/>
    <parent link="bl_lower_link"/>
    <child link="bl_sphere"/>
  </joint>
  <joint name="br_foot" type="fixed">
    <origin rpy="0 0 0" xyz="-0.4 -0.05 0.015"/>
    <parent link="br_lower_link"/>
    <child link="br_sphere"/>
  </joint>

  <!-- Added to avoid warning about base link having inertia -->
  <link name="base"/>
  <joint name="base_to_body" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="base"/>
    <child link="body_link"/>
  </joint>

  <link name="body_link">
