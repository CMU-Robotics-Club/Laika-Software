    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="$(find laika_sim)/meshes/body_link.STL"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="1.4272E-17 6.0167E-18 0.098263"/>
      <mass value="17.196"/>
      <inertia ixx="0.40098" ixy="-4.2352E-18" ixz="-3.2762E-18" iyy="0.10245" iyz="2.0166E-17" izz="0.41797"/>
